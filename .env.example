# GitHub Integration
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your_azure_subscription_id
AZURE_TENANT_ID=your_azure_tenant_id
AZURE_CLIENT_ID=your_azure_client_id
AZURE_CLIENT_SECRET=your_azure_client_secret

# Web Application Configuration
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# GitHub PR Monitor Service
PR_MONITOR_ALLOWED_REPOS=org1/repo1,org2/repo2
PR_MONITOR_WEBHOOK_ENDPOINT=https://your-azure-function-url

# Ethical Reasoning Engine
ETHICAL_REASONING_MODEL_PATH=/path/to/ethical/model
ETHICAL_REASONING_API_KEY=your_ethical_reasoning_api_key

# Logging and Monitoring
SENTRY_DSN=your_sentry_dsn
DATADOG_API_KEY=your_datadog_api_key

# Agent Orchestration
AGENT_ORCHESTRATOR_SECRET=your_agent_orchestrator_secret
AGENT_COMMUNICATION_ENDPOINT=https://your-agent-communication-url

# Development Flags
ENABLE_MOCK_SERVICES=false
DEBUG_MODE=false

# Third-Party Integrations
OPENAI_API_KEY=your_openai_api_key

# Anthropic AI Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-opus-20240229

# Pinecone Vector Database Configuration
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment

# Project Specific
PROJECT_OVERSEER_ENVIRONMENT=development
PROJECT_OVERSEER_VERSION=0.1.0
